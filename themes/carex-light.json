  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#9ca0b0", "fontStyle": "italic" }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": { "foreground": "#40a02b" }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#df8e1d" }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": { "foreground": "#fe640b" }
    },
    {
      "name": "Null",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": { "foreground": "#fe640b" }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": { "foreground": "#d20428" }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": { "foreground": "#d20428" }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#1e66f5" }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": { "foreground": "#8839ef" }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": { "foreground": "#4c4f69" }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property"],
      "settings": { "foreground": "#209fb5" }
    },
    {
      "name": "Constant",
      "scope": ["constant"],
      "settings": { "foreground": "#fe640b" }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#209fb5" }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#d20428" }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#8839ef" }
    }
  ]
}
