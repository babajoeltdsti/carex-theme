  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6c7086", "fontStyle": "italic" }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": { "foreground": "#a6e3a1" }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#f9e2af" }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": { "foreground": "#fab387" }
    },
    {
      "name": "Null",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": { "foreground": "#fab387" }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": { "foreground": "#f38ba8" }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": { "foreground": "#f38ba8" }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#89b4fa" }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": { "foreground": "#cba6f7" }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": { "foreground": "#cdd6f4" }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property"],
      "settings": { "foreground": "#89dceb" }
    },
    {
      "name": "Constant",
      "scope": ["constant"],
      "settings": { "foreground": "#fab387" }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#89dceb" }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#f38ba8" }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#cba6f7" }
